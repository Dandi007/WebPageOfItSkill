		$("#adlayer").hide();
	});
	$("#adlayerclose").click(function(){
		$("#adlayertitle").show();
	});
	
	$("#adlayeropen").click(function(){
		$("#adlayer").show();
		$("#adlayertitle").hide();
	});
});
</script>

